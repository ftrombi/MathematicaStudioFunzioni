   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "-", "1"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{"x", "-", "2"}], ")"}]}], ")"}], 
    FormBox[
     SuperscriptBox["E", 
      RowBox[{"x", "-", "1"}]],
     TraditionalForm]}]}], ";"}]], "Input",
