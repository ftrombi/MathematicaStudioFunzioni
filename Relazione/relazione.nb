Cell[TextData[{
 "Studio della funzione f(x) = ((x-1)/(x-2))",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"x", "-", "1"}]], TraditionalForm]]]
}], "Section",
 TextJustification->1,
 FontFamily->"Times New Roman",
 FontSize->36],

Cell[TextData[{
 " (Ricordatevi di ripulire le variabili f, x, e che la costante \"e\" vuole \
la maiuscola in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ")."
}], "Text",
 FontFamily->"Times New Roman"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"f", ",", "x"}], "]"}], ";"}]], "Input",
 FontFamily->"Times New Roman"],

Cell[BoxData[
