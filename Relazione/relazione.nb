Cell[TextData[{
 "Studio della funzione f(x) = ((x-1)/(x-2))",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"x", "-", "1"}]], TraditionalForm]]]
}], "Section",
 TextJustification->1,
 FontFamily->"Times New Roman",
 FontSize->36],

Cell[TextData[{
 " (Ricordatevi di ripulire le variabili f, x, e che la costante \"e\" vuole \
la maiuscola in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ")."
}], "Text",
 FontFamily->"Times New Roman"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"f", ",", "x"}], "]"}], ";"}]], "Input",
 FontFamily->"Times New Roman"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "-", "1"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{"x", "-", "2"}], ")"}]}], ")"}], 
    FormBox[
     SuperscriptBox["E", 
      RowBox[{"x", "-", "1"}]],
     TraditionalForm]}]}], ";"}]], "Input",
 TextJustification->1,
 FontFamily->"Times New Roman"],

Cell[TextData[{
 StyleBox["Determinare",
  FontWeight->"Bold"],
 ": dominio, grafico, limiti, asintoti, zeri, massimi, minimi, crescenza, \
decrescenza, flessi. Disegnare la tangente nel punto di flesso."
}], "Text",
 TextJustification->1,
 FontFamily->"Times New Roman"],

Cell[TextData[{
 StyleBox["Dominio",
  FontWeight->"Bold"],
 ": tutti i numeri reali tranne x=2"
}], "Text",
 FontFamily->"Times New Roman"],

Cell[TextData[{
 StyleBox["Grafico. ",
  FontWeight->"Bold"],
 "Modificate il dominio della y (comando PlotRange -> {y1,y2}) in modo da \
rendere il grafico pi\[UGrave] comprensibile."
}], "Text",
 FontFamily->"Times New Roman"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"red", " ", "=", " ", 
