Cell[BoxData[
 RowBox[{"SegnoFunzione", "[", 
  RowBox[{"f", ",", "x"}], "]"}]], "Input"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"La funzione \[EGrave] positiva per \"\>", "\[InvisibleSpace]", 
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       SqrtBox["3"], "-", 
       RowBox[{"Log", "[", "5", "]"}], "-", 
       RowBox[{"Log", "[", "7", "]"}]}], ")"}]}], "<", "x", "<", "0"}]}],
  SequenceForm["La funzione \[EGrave] positiva per ", 
   Inequality[
   Rational[1, 2] (3^Rational[1, 2] - Log[5] - Log[7]), Less, $CellContext`x, 
    Less, 0]],
  Editable->False]], "Print",
 CellChangeTimes->{3.673063903522348*^9, 3.6730640983538513`*^9, 
  3.6730647548875847`*^9}]
